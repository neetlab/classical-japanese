{
  "dependencies": {
    "@types/jest": "^27.4.0",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "packageManager": "yarn@3.1.1",
  "jest": {
    "preset": "ts-jest",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/__fixtures__/**"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.spec.ts"
    ]
  }
}
